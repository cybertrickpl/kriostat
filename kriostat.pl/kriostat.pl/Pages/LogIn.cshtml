@page
@model LogInModel
@{
    ViewData["Title"] = "Log in";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to log in.</p>


<form  method="post">
    <br /> 
        <label for="login">Enter login:</label>
    <input type="text" />
    
    <br />
        <label for="password">Enter password:</label>
    <input type="text" />
       
        <button type="submit">Log in</button>

</form>
